{
  "name": "@natlibfi/aleph-x-proxy",
  "description": "A proxy server for enhancing Aleph's X-service",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://github.com/natlibfi/aleph-x-proxy",
  "bugs": {
    "url": "https://github.com/natlibfi/aleph-x-proxy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/aleph-x-proxy.git"
  },
  "license": "MIT",
  "version": "3.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node dist/index.js",
    "lint": "eslint src",
    "lint:dev": "eslint --fix src",
    "build": "esbuild \"src/**/*.js\" --sourcemap --platform=node --outdir=dist",
    "test": "npm run lint",
    "test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec './src/**/*.test.js'",
    "dev:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './src/**/*.test.js'"
  },
  "dependencies": {
    "@natlibfi/melinda-backend-commons": "^3.0.0",
    "esbuild": "^0.27.0",
    "express": "^4.21.2",
    "http-proxy-3": "^1.23.0",
    "http-status": "^2.1.0",
    "into-stream": "^6.0.0",
    "ip-range-check": "^0.2.0",
    "moment": "^2.30.1",
    "oracledb": "^6.10.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jsonpath": "^1.1.1"
  }
}
